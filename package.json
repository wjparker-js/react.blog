{
  "name": "gatsby-blog-cms",
  "version": "1.0.0",
  "description": "A modern blog CMS built with Gatsby, React, TypeScript, and Prisma",
  "private": true,
  "workspaces": [
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "gatsby develop",
    "develop": "gatsby develop",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "gatsby build",
    "start": "npm run start:backend & npm run start:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "gatsby serve",
    "serve": "gatsby serve",
    "clean-gatsby": "gatsby clean",
    "db:setup": "cd backend && npm run db:generate && npm run db:push && npm run db:seed",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed",
    "db:studio": "cd backend && npm run db:studio",
    "db:reset": "cd backend && npm run db:reset",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "install:all": "npm install && cd backend && npm install",
    "clean": "rm -rf node_modules backend/node_modules backend/dist public .cache",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "npm run test:backend",
    "test:backend": "cd backend && npm test",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "npm run type-check:backend",
    "type-check:backend": "cd backend && npm run typecheck"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "gatsby",
    "blog",
    "cms",
    "react",
    "typescript",
    "prisma",
    "mysql",
    "node.js",
    "express"
  ],
  "author": "Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/gatsby-blog-cms.git"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.9.0",
    "disable-scroll": "^0.6.0",
    "gatsby": "^5.14.4",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-styled-components": "^6.14.0",
    "gatsby-plugin-typography": "^5.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-vscode": "^3.3.1",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.23",
    "styled-components": "^6.1.18",
    "styled-reset": "^4.5.2",
    "typescript": "^5.8.3",
    "typography": "^0.16.24"
  }
}
