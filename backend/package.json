    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2"
  },
  "engines": {
    "node": ">=20"
