{
  "name": "gatsby-blog-cms-backend",
  "version": "1.0.0",
  "description": "Backend API for Gatsby Blog CMS with Prisma",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate dev",
